{
   "background": {
      "persistent": false,
      "scripts": [
         "background.js"
      ]
   },
   "author": "Luis Fontes",
   "content_security_policy": "default-src 'self'",
   "description": "Redirect aliases for URL addresses",
   "homepage_url": "https://github.com/luisfontes19/chrome-url-redirector",
   "browser_action": {
      "default_icon": {
         "128": "icon-128.png",
         "96": "icon-96.png",
         "48": "icon-48.png",
         "16": "icon-16.png"
      },
      "default_title": "url alias",
      "default_popup": "options.html"
   },
   "icons": {
      "128": "icon-128.png",
      "96": "icon-96.png",
      "48": "icon-48.png",
      "16": "icon-16.png"
   },
   "manifest_version": 2,
   "name": "Url Redirector",
   "options_page": "options.html",
   "permissions": [
      "webNavigation",
      "storage"
   ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.0.0"
}